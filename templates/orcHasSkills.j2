{% extends 'layout.html' %}

{% block body %}
<table class="center">
    <thead>
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Skill</th>
        <th>Skill Level</th>
    </tr>
    <tbody>
    {% for item in orc_skills %}
    <tr>
        <td>{{item['orc_skill_id']}}</td>
        <td>{{item['first_name']}}</td>
        <td>{{item['last_name']}}</td>
        <td>{{item['skill']}}</td>
        <td>{{item['skill_level']}}</td>
    </tr>
    {% endfor %}
    </table>
{% endblock%}